  display: flex;
  justify-content: center;
  align-items: center;
  height: 100rpx;
  color: #5770bd;
  text-decoration: underline;
  font-size: 30rpx;
}
.container.data-v-0f00adf4 {
  padding-bottom: 134rpx;
  /* 空白页 */
}
.container .empty.data-v-0f00adf4 {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  padding-bottom: 100rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  background: #fff;
}
.container .empty image.data-v-0f00adf4 {
  width: 240rpx;
  height: 160rpx;
  margin-bottom: 30rpx;
}
.container .empty .empty-tips.data-v-0f00adf4 {
  display: flex;
  font-size: 26rpx;
  color: #C0C4CC;
}
.container .empty .empty-tips .navigator.data-v-0f00adf4 {
  color: #5770bd;
  margin-left: 16rpx;
}
/* 购物车列表项 */
.cart-item.data-v-0f00adf4 {
  display: flex;
  position: relative;
  padding: 30rpx 40rpx;
}
.cart-item .image-wrapper.data-v-0f00adf4 {
  width: 200rpx;
  height: 200rpx;
  flex-shrink: 0;
  position: relative;
}
.cart-item .image-wrapper image.data-v-0f00adf4 {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
}
.cart-item .checkbox.data-v-0f00adf4 {
  position: absolute;
  left: -16rpx;
  top: -16rpx;
  z-index: 8;
  font-size: 44rpx;
  line-height: 1;
  padding: 4rpx;
  color: #C0C4CC;
  background: #fff;
  border-radius: 50px;
}
.cart-item .item-right.data-v-0f00adf4 {
  display: flex;
  flex-direction: column;
  flex: 1;
  overflow: hidden;
  position: relative;
  padding-left: 30rpx;
}
.cart-item .item-right .title.data-v-0f00adf4, .cart-item .item-right .price.data-v-0f00adf4 {
  /* font-size:$font-base + 2upx; */
  color: #303133;
  height: 40rpx;
  line-height: 40rpx;
}
.cart-item .item-right .attr.data-v-0f00adf4 {
  font-size: 24rpx;
  color: #909399;
  height: 40rpx;
  line-height: 40rpx;
}
.cart-item .item-right .price.data-v-0f00adf4 {
  height: 50rpx;
  line-height: 50rpx;
}
.cart-item .del-btn.data-v-0f00adf4 {
  padding: 4rpx 10rpx;
  font-size: 34rpx;
  height: 50rpx;
  color: #909399;
}
/* 底部栏 */
.action-section-w.data-v-0f00adf4 {
  position: fixed;
  left: 30rpx;
  bottom: 120rpx;
  z-index: 95;
  width: 100%;
}
.action-section-w .action-section.data-v-0f00adf4 {
  display: flex;
  align-items: center;
  width: 690rpx;
  padding: 0 30rpx;
  height: 100rpx;
  background: rgba(255, 255, 255, 0.9);
  box-shadow: 0 0 20rpx 0 rgba(0, 0, 0, 0.5);
  border-radius: 16rpx;
}
.action-section-w .action-section .checkbox.data-v-0f00adf4 {
  height: 52rpx;
  position: relative;
  z-index: 5;
}
.action-section-w .action-section .checkbox .img-bg.data-v-0f00adf4 {
  background-color: #fff;
  border-radius: 50%;
  width: 52rpx;
  height: 100%;
  position: relative;
  z-index: 5;
}
.action-section-w .action-section .checkbox image.data-v-0f00adf4 {
  width: 52rpx;
  height: 100%;
  position: relative;
  z-index: 5;
}
.action-section-w .action-section .clear-btn.data-v-0f00adf4 {
  position: absolute;
  left: 26rpx;
  top: 0;
  z-index: 4;
  width: 0;
  height: 52rpx;
  line-height: 52rpx;
  padding-left: 38rpx;
  font-size: 28rpx;
  color: #fff;
  background: #C0C4CC;
  border-radius: 0 50px 50px 0;
  opacity: 0;
  transition: .2s;
}
.action-section-w .action-section .clear-btn.show.data-v-0f00adf4 {
  opacity: 1;
  width: 120rpx;
}
.action-section-w .action-section .total-box.data-v-0f00adf4 {
  flex: 1;
  display: flex;
  flex-direction: column;
  text-align: right;
  padding-right: 40rpx;
}
.action-section-w .action-section .total-box .price.data-v-0f00adf4 {
  font-size: 32rpx;
  color: #303133;
  font-weight: bold;
}
.action-section-w .action-section .total-box .coupon.data-v-0f00adf4 {
  font-size: 24rpx;
  color: #909399;
}
.action-section-w .action-section .total-box .coupon text.data-v-0f00adf4 {
  color: #303133;
}
.action-section-w .action-section .confirm-btn.data-v-0f00adf4 {
  padding: 0 38rpx;
  margin: 0;
  border-radius: 100px;
  height: 76rpx;
  line-height: 76rpx;
  font-size: 30rpx;
  background: #5770bd;
  /* box-shadow: 1px 2px 5px rgba(217, 60, 93, 0.72) */
}
/* 复选框选中状态 */
.action-section .checkbox.checked.data-v-0f00adf4,
.cart-item .checkbox.checked.data-v-0f00adf4 {
  color: #5770bd;
}

